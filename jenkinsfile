    agent any
        stages{
            stage("build code")
            {
                echo "build is success"
            }
             stage("test code")
            {
                echo "test is success"
            }
             stage("code analysis code")
            {
                echo "code analysis is success"
            }
             stage("deploy")
            {
                echo "deploy is success"
            }

        }
}